{
    "builds": [{
        "src": "exercicio/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdasize": "15mb", "runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "exercicio/wsgi.py"
        },
        {
            "src": "/templates/static/(.*)",
            "dest": "exercicio/static/$1"
        }
    ]
}
